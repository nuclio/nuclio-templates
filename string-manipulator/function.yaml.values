manipulationKind:
  displayName: Manipulation Kind
  kind: choice
  description: "What do to with the incoming request body"
  required: true
  attributes:
    choices: [echo, reverse, md5]
    defaultValue: echo

maxWorkers:
  displayName: Number of workers
  kind: number
  description: "Number of requests to handle concurrently"
  required: true
  attributes:
    defaultValue: 4
    minValue: 1
    maxValue: 10
